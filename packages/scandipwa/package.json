{
    "name": "@scandipwa/scandipwa",
    "description": "ScandiPWA source code used by CSA.",
    "version": "5.0.6",
    "scripts": {
        "start": "scandipwa-scripts start",
        "start:magento": "BUILD_MODE=magento scandipwa-scripts start",
        "build": "scandipwa-scripts build",
        "build:magento": "BUILD_MODE=magento scandipwa-scripts build",
        "sb": "start-storybook -p 6006 --ci",
        "build-sb": "build-storybook"
    },
    "dependencies": {
        "@babel/preset-env": "^7.15.0",
        "@scandipwa/chunk-optimizer": "0.0.3",
        "@scandipwa/m2-theme": "0.2.2",
        "@scandipwa/next-emulator": "0.0.3",
        "@scandipwa/postcss-config": "0.0.2",
        "@scandipwa/scandipwa-scripts": "^2.4.18",
        "@scandipwa/service-worker": "0.0.17",
        "@scandipwa/webpack-i18n-runtime": "0.0.29",
        "@tilework/opus": "^0.0.9",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/react-router": "^5.1.16",
        "@types/react-router-dom": "^5.1.8",
        "@types/webpack-env": "^1.16.2",
        "history": "^4.9.0",
        "html-react-parser": "^0.13.0",
        "idb": "^6.1.4",
        "intersection-observer": "^0.12.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-input-range": "^1.3.0",
        "react-intersection-observer": "^8.31.0",
        "react-redux": "^7.2.0",
        "react-router": "^5.2.1",
        "react-router-dom": "^5.2.1",
        "react-stripe-elements": "^6.1.2",
        "react-vimeo": "^2.0.0",
        "react-youtube": "^7.11.2",
        "react-zoom-pan-pinch": "^1.6.1",
        "rebem-classname": "^0.4.0",
        "redux": "^4.0.5",
        "unstated": "^2.1.1",
        "workbox-webpack-plugin": "^6.3.0"
    },
    "scandipwa": {
        "themeAlias": "Source",
        "type": "theme",
        "sourceDirectories": [
            "./.storybook"
        ],
        "locales": {
            "en_US": true,
            "da_DK": true,
            "de_DE": true,
            "es_ES": true,
            "fa_IR": true,
            "fr_FR": true,
            "fi_FI": true,
            "it_IT": true,
            "lv_LV": true,
            "nb_NO": true,
            "nl_NL": true,
            "pl_PL": true,
            "pt_BR": true,
            "ru_RU": true,
            "sl_SI": true,
            "sv_SE": true,
            "tr_TR": true,
            "zh_TW": true
        },
        "extensions": {
            "@scandipwa/m2-theme": true,
            "@scandipwa/webpack-i18n-runtime": true,
            "@scandipwa/service-worker": true,
            "@scandipwa/postcss-config": true,
            "@scandipwa/chunk-optimizer": true,
            "@scandipwa/next-emulator": false
        },
        "composer": {
            "magento/framework": "*",
            "scandipwa/service-worker": "1.0.2",
            "scandipwa/installer": "4.0",
            "scandipwa/menu-organizer": "2.1.4",
            "scandipwa/persisted-query": "3.0.2",
            "scandipwa/slider-graphql": "2.1.0",
            "scandipwa/cms-graphql": "1.4.4",
            "scandipwa/catalog-graphql": "3.2.3",
            "scandipwa/route717": "2.0.1",
            "scandipwa/performance": "1.5.1",
            "scandipwa/customer-graph-ql": "3.1.3",
            "scandipwa/quote-graphql": "2.19.15",
            "scandipwa/wishlist-graphql": "2.0.13",
            "scandipwa/urlrewrite-graphql": "1.3.7",
            "scandipwa/store-graphql": "1.2.5",
            "scandipwa/customization": "1.5.8",
            "scandipwa/cache": "1.1.4",
            "scandipwa/locale": "2",
            "scandipwa/contact-graphql": "1.0.0",
            "scandipwa/klarna-graphql": "1.1.2",
            "scandipwa/compare-graphql": "1.0.8",
            "scandipwa/customer-downloadable-graphql": "1.0.2",
            "scandipwa/directory-graphql": "1.0.1",
            "scandipwa/product-alerts-graphql": "1.0.0",
            "scandipwa/catalog-customer-graphql": "1.0.0"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": [
            "@scandipwa"
        ],
        "rules": {
            "@scandipwa/scandipwa-guidelines/use-namespace": "error",
            "@scandipwa/scandipwa-guidelines/derived-class-names": "warn",
            "@scandipwa/scandipwa-guidelines/only-render-in-component": "off",
            "@scandipwa/scandipwa-guidelines/jsx-no-props-destruction": "off",
            "@scandipwa/scandipwa-guidelines/jsx-no-conditional": "off",
            "@scandipwa/scandipwa-guidelines/use-magic-construct": "warn",
            "@scandipwa/scandipwa-guidelines/no-jsx-variables": "off",
            "@scandipwa/scandipwa-guidelines/use-license": "off",
            "react/jsx-no-useless-fragment": "off",
            "max-classes-per-file": "off",
            "max-lines": "off",
            "react/prop-types": "off",
            "consistent-return": "off",
            "fp/no-let": "off",
            "react/boolean-prop-naming": "off",
            "@typescript-eslint/no-empty-interface": "off"
        },
        "overrides": [
            {
                "files": [
                    "./src/query/**.query.+(ts|js)"
                ],
                "rules": {
                    "@typescript-eslint/explicit-module-boundary-types": "off"
                }
            },
            {
                "files": [
                    "./src/component/*Icon/**.+(ts|js)"
                ],
                "rules": {
                    "max-len": "off"
                }
            },
            {
                "files": [
                    "./src/**/**.d.ts"
                ],
                "rules": {
                    "no-use-before-define": "off"
                }
            }
        ],
        "env": {
            "browser": true
        },
        "globals": {
            "globalThis": true
        }
    },
    "license": "OSL-3.0",
    "publishConfig": {
        "access": "public"
    },
    "proxy": "https://demo100-ors-1588667385-csa-qnb.scandipwa.cloud/",
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@storybook/addon-actions": "^6.3.8",
        "@storybook/addon-essentials": "^6.3.8",
        "@storybook/addon-links": "^6.3.8",
        "@storybook/preset-scss": "^1.0.3",
        "@storybook/react": "^6.3.8",
        "babel-loader": "^8.2.2",
        "sass-loader": "10"
    }
}
